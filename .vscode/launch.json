    "configurations": [
        {
            "name": "Launch with GDB",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceRoot}/build/hades.elf",
            "target": ":1234",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",

            "preLaunchTask": "Launch QEMU (No KVM)"
        }

    ]
